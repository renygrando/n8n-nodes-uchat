{
        "name": "n8n-nodes-uchat",
        "version": "0.4.2",
        "description": "n8n node para integração com a API do uChat",
        "license": "MIT",
        "homepage": "https://www.uchat.com.au",
        "keywords": [
                "n8n-community-node-package",
                "uchat",
                "whatsapp",
                "chat",
                "automation"
        ],
        "author": {
                "name": "uChat",
                "email": "contato@uchat.com.au"
        },
        "repository": {
                "type": "git",
                "url": "https://github.com/renygrando/n8n-nodes-uchat.git"
        },
        "bugs": {
                "url": "https://github.com/renygrando/n8n-nodes-uchat/issues"
        },
        "scripts": {
                "build": "n8n-node build",
                "build:watch": "tsc --watch",
                "dev": "n8n-node dev",
                "lint": "n8n-node lint",
                "lint:fix": "n8n-node lint --fix",
                "release": "n8n-node release",
                "prepublishOnly": "n8n-node prerelease"
        },
        "files": [
                "dist"
        ],
        "n8n": {
                "n8nNodesApiVersion": 1,
                "strict": true,
                "credentials": [
                        "dist/credentials/UchatApi.credentials.js"
                ],
                "nodes": [
                        "dist/nodes/Uchat/Uchat.node.js",
                        "dist/nodes/Uchat/UchatTrigger.node.js"
                ]
        },
        "devDependencies": {
                "@n8n/node-cli": "*",
                "eslint": "9.32.0",
                "prettier": "3.6.2",
                "release-it": "^19.0.4",
                "typescript": "5.9.2"
        },
        "peerDependencies": {
                "n8n-workflow": "*"
        }
}
