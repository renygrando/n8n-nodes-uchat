{
        "name": "n8n-nodes-uchat",
        "version": "0.4.7-1",
        "description": "n8n node para integração com a API do uChat",
        "keywords": [
                "n8n-community-node-package",
                "uchat",
                "whatsapp",
                "chat",
                "automation"
        ],
        "license": "MIT",
        "homepage": "https://www.uchat.com.au",
        "author": {
                "name": "uChat",
                "email": "contato@uchat.com.au"
        },
        "repository": {
                "type": "git",
                "url": "git+https://github.com/renygrando/n8n-nodes-uchat.git"
        },
        "main": "dist/index.js",
        "types": "dist/index.d.ts",
        "scripts": {
                "build": "tsc",
                "dev": "tsc --watch",
                "format": "prettier --write .",
                "lint": "eslint .",
                "lintfix": "eslint . --fix",
                "prepublishOnly": "npm run build"
        },
        "files": [
                "dist"
        ],
        "n8n": {
                "n8nNodesApiVersion": 1,
                "credentials": [
                        "dist/credentials/UchatApi.credentials.js"
                ],
                "nodes": [
                        "dist/nodes/Uchat/Uchat.node.js",
                        "dist/nodes/Uchat/UchatTrigger.node.js"
                ]
        },
        "devDependencies": {
                "@n8n/node-cli": "^0.14.0",
                "@types/express": "^4.17.17",
                "@types/node": "^18.16.16",
                "eslint": "^9.39.1",
                "typescript": "~5.0.4"
        },
        "dependencies": {
                "axios": "^1.4.0",
                "n8n-core": "^1.0.0",
                "n8n-workflow": "^1.0.0"
        }
}
